{
  "editor.cursorBlinking": "solid",
  "editor.codeActionsOnSave": {},
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<Left>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<Right>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Space>", "w"],
      "after": [":", "w", "<Enter>"]
    }
  ],
  "terminal.integrated.sendKeybindingsToShell": true,
  "editor.tabSize": 2,
  "redhat.telemetry.enabled": false,
  "explorer.confirmDelete": false,
  "editor.fontFamily": "'LigaConsolas Nerd Font Book', 'Fira Code', 'JetBrainsMono Nerd Font Mono', 'Monaco Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "cmake.showOptionsMovedNotification": false,
  "json.schemas": [],
  "notebook.output.scrolling": true,
  "editor.indentSize": "tabSize",
  "[cpp]": {
    "editor.tabSize": 2,
    "editor.detectIndentation": true,
    "editor.dragAndDrop": false,
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "editor.dragAndDrop": false,
    "editor.defaultFormatter": "eeyore.yapf"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "right"
  },
  "notebook.lineNumbers": "on",
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "markdown-preview-enhanced.codeBlockTheme": "one-dark.css",
  "markdown-preview-enhanced.revealjsTheme": "black.css",
  "terminal.integrated.defaultProfile.linux": "fish",
  "leetcode.workspaceFolder": "/home/ian/.leetcode",
  "C_Cpp.default.compilerPath": "/usr/bin/g++",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "leetcode.defaultLanguage": "cpp",
  "leetcode.hint.commandShortcut": false,
  "editor.defaultFormatter": "ms-vscode.cpptools",
  "editor.defaultFoldingRangeProvider": "ms-vscode.cpptools",
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2}",
  "leetcode.filePath": {
    "default": {
      "folder": "",
      "filename": "${id}.${kebab-case-name}.${ext}"
    }
  },
  "haskell.manageHLS": "GHCup",
  "C_Cpp.intelliSenseEngine": "disabled",
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "explorer.confirmDragAndDrop": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "window.zoomLevel": 0.4,
  "explorer.compactFolders": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.linkedEditing": true,
  "workbench.iconTheme": "a-file-icon-vscode",
  "editor.fontSize": 14,
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "svelte.enable-ts-plugin": true,
  "C_Cpp.default.compileCommands": [""],
  "C_Cpp.default.customConfigurationVariables": {},
  "diffEditor.ignoreTrimWhitespace": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codeium.enableCommandSideHintEducation": false,
  "codeium.enableConfig": {
    "*": true
  },
  "codeium.disableSupercomplete": true,
  "masmtasm.ASM.emulator": "jsdos",
  "masmtasm.ASM.assembler": "MASM-v6.11",
  "editor.inlayHints.enabled": "off",
  "masmtasm.ASM.actions": {
    "TASM": {
      "baseBundle": "<built-in>/TASM.jsdos",
      "before": ["set PATH=C:\\TASM"],
      "run": ["TASM ${file}", "TLINK ${filename}", ">${filename}"],
      "debug": [
        "TASM /zi ${file}",
        "TLINK /v/3 ${filename}.obj",
        "copy C:\\TASM\\TDC2.TD TDCONFIG.TD",
        "TD -cTDCONFIG.TD ${filename}.exe"
      ]
    },
    "MASM-v6.11": {
      "baseBundle": "<built-in>/MASM-v6.11.jsdos",
      "before": ["set PATH=C:\\MASM"],
      "run": ["masm ${file};", "link ${filename};", ">${filename}"],
      "debug": [
        "masm ${file};",
        "link ${filename}.OBJ;",
        ">debug ${filename}.exe"
      ]
    },
    "MASM-v5.00": {
      "baseBundle": "<built-in>/MASM-v5.00.jsdos",
      "before": ["set PATH=C:\\MASM"],
      "run": ["masm ${file};", "link ${filename};", ">${filename}"],
      "debug": [
        "masm ${file};",
        "link ${filename}.OBJ;",
        ">debug ${filename}.exe"
      ],
      "support": ["jsdos", "dosbox", "dosbox-x", "msdos player"]
    }
  },
  "python.analysis.typeCheckingMode": "standard",
  // "editor.fontWeight": "450",
  "editor.cursorStyle": "normal",
  "markdown-pdf.highlightStyle": "atom-one-dark.css",
  "markdown-pdf.styles": ["/home/ian/Documents/a.css"],
  "python.defaultInterpreterPath": "/bin/python",
  "codesnap.containerPadding": "0em",
  "codesnap.showLineNumbers": false,
  "codesnap.showWindowControls": false,
  "codesnap.target": "window",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "makefile.configureOnOpen": true,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "docker.extension.enableComposeLanguageServer": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  }
}
